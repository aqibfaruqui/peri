.section .text
.global add
add:
    addi sp, sp, -16
    sw ra, 12(sp)
    mv t0, a0
    mv t1, a1
    mv a0, t0
    lw ra, 12(sp)
    addi sp, sp, 16
    ret

.section .text
.global main
main:
    addi sp, sp, -16
    sw ra, 12(sp)
    li t0, 5
    li t1, 10
    mv a0, t0
    mv a1, t1
    call add
    mv t2, a0
    mv a0, t2
    lw ra, 12(sp)
    addi sp, sp, 16
    ret
