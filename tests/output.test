.section .text
.global enable_timer
enable_timer:
    addi sp, sp, -16
    sw ra, 12(sp)
    li t0, 1
    li t1, 0x40000000
    sw t0, 0(t1)
    li t1, 0x40000004
    lw t0, 0(t1)
    mv a0, t0
    lw ra, 12(sp)
    addi sp, sp, 16
    ret

.section .text
.global main
main:
    addi sp, sp, -16
    sw ra, 12(sp)
    call enable_timer
    mv t0, a0
    lw ra, 12(sp)
    addi sp, sp, 16
    ret

